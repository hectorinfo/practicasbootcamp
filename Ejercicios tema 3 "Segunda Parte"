public class Main {
    public static void main(String[] args) {
        Coche micoche = new Coche();
        System.out.println("El numero de puertas es: " +  micoche.npuertas);
        micoche.sumarp();
    }
}
public class Coche {
    int npuertas = 5;
    public void sumarp (){
        this.npuertas++;
        System.out.print("El numero de puertas incrementadas: " + this.npuertas);
    }
}
